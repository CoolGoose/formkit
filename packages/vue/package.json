{
  "name": "@formkit/vue",
  "version": "1.6.5",
  "type": "module",
  "description": "Build industry leading Vue forms 10x faster.",
  "main": "dist/index.cjs",
  "types": "dist/index.d.cts",
  "exports": {
    "./package.json": "./package.json",
    "./core": {
      "import": {
        "types": "./dist/passthru/core/index.d.mts",
        "development": "./dist/passthru/core/index.dev.mjs",
        "default": "./dist/passthru/core/index.mjs"
      },
      "require": {
        "types": "./dist/passthru/core/index.d.cts",
        "default": "./dist/passthru/core/index.cjs"
      }
    },
    "./i18n/locales/*": {
      "import": {
        "types": "./dist/passthru/i18n/locales/*.d.mts",
        "development": "./dist/passthru/i18n/locales/*.dev.mjs",
        "default": "./dist/passthru/i18n/locales/*.mjs"
      },
      "require": {
        "types": "./dist/passthru/i18n/locales/*.d.cts",
        "default": "./dist/passthru/i18n/locales/*.cjs"
      }
    },
    "./i18n": {
      "import": {
        "types": "./dist/passthru/i18n/index.d.mts",
        "development": "./dist/passthru/i18n/index.dev.mjs",
        "default": "./dist/passthru/i18n/index.mjs"
      },
      "require": {
        "types": "./dist/passthru/i18n/index.d.cts",
        "default": "./dist/passthru/i18n/index.cjs"
      }
    },
    "./i18n/i18n": {
      "import": {
        "types": "./dist/passthru/i18n/i18n.d.mts",
        "development": "./dist/passthru/i18n/i18n.dev.mjs",
        "default": "./dist/passthru/i18n/i18n.mjs"
      },
      "require": {
        "types": "./dist/passthru/i18n/i18n.d.cts",
        "default": "./dist/passthru/i18n/i18n.cjs"
      }
    },
    "./icons": {
      "import": {
        "types": "./dist/passthru/icons/index.d.mts",
        "development": "./dist/passthru/icons/index.dev.mjs",
        "default": "./dist/passthru/icons/index.mjs"
      },
      "require": {
        "types": "./dist/passthru/icons/index.d.cts",
        "default": "./dist/passthru/icons/index.cjs"
      }
    },
    "./inputs": {
      "import": {
        "types": "./dist/passthru/inputs/index.d.mts",
        "development": "./dist/passthru/inputs/index.dev.mjs",
        "default": "./dist/passthru/inputs/index.mjs"
      },
      "require": {
        "types": "./dist/passthru/inputs/index.d.cts",
        "default": "./dist/passthru/inputs/index.cjs"
      }
    },
    "./rules": {
      "import": {
        "types": "./dist/passthru/rules/index.d.mts",
        "development": "./dist/passthru/rules/index.dev.mjs",
        "default": "./dist/passthru/rules/index.mjs"
      },
      "require": {
        "types": "./dist/passthru/rules/index.d.cts",
        "default": "./dist/passthru/rules/index.cjs"
      }
    },
    "./themes": {
      "import": {
        "types": "./dist/passthru/themes/index.d.mts",
        "development": "./dist/passthru/themes/index.dev.mjs",
        "default": "./dist/passthru/themes/index.mjs"
      },
      "require": {
        "types": "./dist/passthru/themes/index.d.cts",
        "default": "./dist/passthru/themes/index.cjs"
      }
    },
    "./utils": {
      "import": {
        "types": "./dist/passthru/utils/index.d.mts",
        "development": "./dist/passthru/utils/index.dev.mjs",
        "default": "./dist/passthru/utils/index.mjs"
      },
      "require": {
        "types": "./dist/passthru/utils/index.d.cts",
        "default": "./dist/passthru/utils/index.cjs"
      }
    },
    "./validation": {
      "import": {
        "types": "./dist/passthru/validation/index.d.mts",
        "development": "./dist/passthru/validation/index.dev.mjs",
        "default": "./dist/passthru/validation/index.mjs"
      },
      "require": {
        "types": "./dist/passthru/validation/index.d.cts",
        "default": "./dist/passthru/validation/index.cjs"
      }
    },
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "development": "./dist/index.dev.mjs",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "iife": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.iife.js"
      }
    }
  },
  "keywords": [
    "vue",
    "forms",
    "inputs",
    "validation"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/formkit/formkit.git",
    "directory": "packages/rules"
  },
  "contributors": [
    "Justin Schroeder <justin@formkit.com>"
  ],
  "license": "MIT",
  "scripts": {
    "test": "jest"
  },
  "dependencies": {
    "@formkit/core": "^1.6.5",
    "@formkit/dev": "^1.6.5",
    "@formkit/i18n": "^1.6.5",
    "@formkit/inputs": "^1.6.5",
    "@formkit/icons": "^1.6.5",
    "@formkit/observer": "^1.6.5",
    "@formkit/rules": "^1.6.5",
    "@formkit/themes": "^1.6.5",
    "@formkit/utils": "^1.6.5",
    "@formkit/validation": "^1.6.5"
  },
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@formkit/icons": "workspace:^"
  }
}
