{
  "name": "@formkit/unplugin",
  "type": "module",
  "version": "1.6.2",
  "packageManager": "pnpm@8.6.12",
  "description": "Automatic FormKit configuration ðŸª„",
  "license": "MIT",
  "homepage": "https://github.com/formkit/unplugin-formkit#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/formkit/unplugin-formkit.git"
  },
  "bugs": {
    "url": "https://github.com/formkit/unplugin-formkit/issues"
  },
  "keywords": [
    "formkit",
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "esbuild",
    "transform"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.mts",
      "import": "./dist/vite.mjs",
      "require": "./dist/vite.cjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.mts",
      "import": "./dist/webpack.mjs",
      "require": "./dist/webpack.cjs"
    },
    "./rollup": {
      "types": "./dist/rollup.d.mts",
      "import": "./dist/rollup.mjs",
      "require": "./dist/rollup.cjs"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.mts",
      "import": "./dist/esbuild.mjs",
      "require": "./dist/esbuild.cjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "npm -C playground run dev"
  },
  "peerDependenciesMeta": {
    "webpack": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    }
  },
  "dependencies": {
    "@babel/generator": "^7.24.4",
    "@babel/parser": "^7.24.4",
    "@babel/template": "^7.24.0",
    "@babel/traverse": "^7.24.1",
    "@formkit/inputs": "1.6.2",
    "@formkit/utils": "1.6.2",
    "@types/babel__traverse": "^7.20.5",
    "@vue/compiler-dom": "^3.4.21",
    "consola": "^3.2.3",
    "pathe": "^1.1.1",
    "recast": "^0.23.6",
    "unplugin": "^1.10.0"
  },
  "devDependencies": {
    "@types/babel__generator": "^7.6.8",
    "@types/babel__template": "^7.4.4",
    "@types/node": "^20.5.2",
    "@vitejs/plugin-vue": "^4.6.2",
    "esbuild": "^0.20.2",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "esno": "^0.17.0",
    "fast-glob": "^3.3.1",
    "nodemon": "^3.0.1",
    "unplugin-file-url": "^0.1.1",
    "vite": "^5.0.4",
    "vite-plugin-inspect": "^0.8.1",
    "vue": "^3.4.21",
    "webpack": "^5.88.2"
  },
  "peerDependencies": {
    "@formkit/vue": "^1.6.2"
  }
}
